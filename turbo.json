{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["types"],
			"inputs": ["$TURBO_DEFAULT", ".env*"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"]
		},
		"types": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"lint": {},
		"test": {
			"dependsOn": ["build"],
			"inputs": ["src/**/*.ts", "src/**/*.tsx", "test/**/*.ts", "test/**/*.tsx"],
			"outputs": []
		},
		"test:r": {
			"dependsOn": ["build"],
			"inputs": ["src/**/*.ts", "src/**/*.tsx", "test/**/*.ts", "test/**/*.tsx"],
			"outputs": []
		}
	}
}
