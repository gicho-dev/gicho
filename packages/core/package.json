{
	"name": "@gicho/core",
	"version": "0.0.1-alpha.1",
	"description": "A collection of base utilities for happy coding with TypeScript.",
	"keywords": [
		"base",
		"basis",
		"core",
		"foundation",
		"happy-coding",
		"modular",
		"toolkit",
		"typescript",
		"utilities",
		"utils"
	],
	"license": "MIT",
	"author": "Lucas Choi",
	"repository": {
		"type": "git",
		"url": "https://github.com/gicho-dev/gicho",
		"directory": "packages/core"
	},
	"homepage": "https://github.com/gicho-dev/gicho",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"source": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./fetch": {
			"source": "./src/fetch/index.ts",
			"types": "./dist/fetch.d.ts",
			"default": "./dist/fetch.js"
		},
		"./is": {
			"source": "./src/is/index.ts",
			"types": "./dist/is.d.ts",
			"default": "./dist/is.js"
		},
		"./object": {
			"source": "./src/object/index.ts",
			"types": "./dist/object.d.ts",
			"default": "./dist/object.js"
		},
		"./string": {
			"source": "./src/string/index.ts",
			"types": "./dist/string.d.ts",
			"default": "./dist/string.js"
		},
		"./types": {
			"source": "./src/types/index.ts",
			"types": "./dist/types.d.ts",
			"default": "./dist/types.js"
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"engines": {
		"node": ">=22"
	},
	"scripts": {
		"b": "tsdown",
		"build": "tsdown",
		"coverage": "vitest --coverage",
		"format": "prettier --check .",
		"format:fix": "prettier --check --write .",
		"lint": "eslint . --cache",
		"lint:fix": "pnpm lint -- --fix",
		"test": "vitest",
		"test:b": "vitest bench",
		"test:r": "vitest run",
		"test:w": "vitest watch",
		"types": "tsc --noEmit"
	},
	"dependencies": {},
	"devDependencies": {},
	"peerDependencies": {},
	"publishConfig": {
		"exports": {
			".": "./dist/index.js",
			"./fetch": "./dist/fetch.js",
			"./is": "./dist/is.js",
			"./object": "./dist/object.js",
			"./string": "./dist/string.js",
			"./types": "./dist/types.js",
			"./package.json": "./package.json"
		}
	}
}
